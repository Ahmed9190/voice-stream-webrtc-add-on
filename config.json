{
  "name": "Voice Stream WebRTC Add-on",
  "version": "1.0.0",
  "slug": "voice_stream_webrtc",
  "description": "A WebRTC streaming server add-on written in Rust.",
  "url": "https://github.com/Ahmed9190/home-assistant-docker-add-on",
  "startup": "services",
  "boot": "auto",
  "arch": ["aarch64", "amd64"],
  "options": {
    "log_level": "info"
  },
  "schema": {
    "log_level": "str"
  },
  "ports": {
    "8080/tcp": 8080,
    "8081/tcp": 8081
  },
  "build_from": {
    "aarch64": "ghcr.io/home-assistant/aarch64-base:latest",
    "amd64": "ghcr.io/home-assistant/amd64-base:latest"
  },
  "image": "ghcr.io/ahmed9190/voice-stream-webrtc-{arch}"
}
